{
  "expo": {
    "name": "Pomodoro",
    "slug": "pomodoro",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#EB5757"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#EB5757"
      },
      "package": "com.nickvogrinec.pomodoro",
      "versionCode": 2
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon-notifications.png"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "74ce5d18-9dbc-4f1a-8e11-d5fbff7ad25a"
      }
    },
    "owner": "darkpwnss"
  }
}
